    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch Executable",
            "type": "go",
            "request": "launch",
            "preLaunchTask": "build",
            "mode": "exec",
            "program": "${workspaceFolder}/dist/${workspaceFolderBasename}",
            "env": {},
            "args": []
        },
        {
            "name": "Launch Debug",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${file}",
            "env": {},
            "args": []
        },
        {
            "name": "Launch Run",
            "preLaunchTask": "run",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}"
        }
    ]
}